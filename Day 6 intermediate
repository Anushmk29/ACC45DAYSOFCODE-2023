#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	string h;
	cin>>t;
	while(t--)
	{
	    bool flag = false;
	    cin>>h>>n;
	     for(int i=0;i<n;i++)
	     {
	         if(h[i]==5 or h[i]==0)
	         {
	        flag=true;
	         break;
	             
	         }
	     }
	     if(flag){cout << "yes" << endl;}
	     else {cout<<"no"<<endl;}
	}
	return 0;
}
